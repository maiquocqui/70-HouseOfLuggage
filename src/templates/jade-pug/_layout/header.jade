					a(href="#") <i class="fa fa-map-marker" aria-hidden="true"></i>
						span Hệ thống
				.wishlist
					a(href="#") <i class="fa fa-heart" aria-hidden="true"></i>
						span Wish list
				.login
					ul
						li
							a(href="#") <i class="fa fa-user" aria-hidden="true"></i>
								span Đăng ký
						li 
							a(href="#") <i class="fa fa-unlock-alt" aria-hidden="true"></i>
								span Đăng nhập
