					a(href="javascript:;" data-fancybox="contact" data-src="#popup-contact") <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
						span Đăng ký làm đại lý
					#popup-contact.popup-contact
						.popup-wrap
							.row
								.col-12.col-md-6
									.img
										img(src="img/popup.jpg", alt="")
								.col-12.col-md-6
									.popup-form
										.module-title LIÊN HỆ HỢP TÁC
										.form-group
											input(type="text" placeholder="Họ tên *")
										.form-group
											input(type="text" placeholder="Số điện thoại *")
										.form-group
											input(type="text" placeholder="Email *")
										.form-group
											select
												option(value="") Chọn tỉnh / thành phố
										.form-group.captcha
											input(type="text" placeholder="Nhập mã hiển thị")
										input(type="submit", value="Gửi")
